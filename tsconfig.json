{
    "compilerOptions": {
        "outDir": "./dist",
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "target": "es6"
    },
    "include": [
        "app.ts",
        "models/*",
        "repositories/*",
        "services/*",
        "controllers/*"
    ],
    "exclude": [
        "node_modules/*",
        "services-mock/*",
        "*.spec.ts",
    ]
}